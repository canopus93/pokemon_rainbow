          <div id="top_ten_pokemon_with_biggest_total_damage"></div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="panel panel-primary">
    	<div class="panel-heading">
        <a data-toggle="collapse" href="#collapse_top_ten_pokedex_average_level">Top Ten Pokedex Average Level</a>
      </div>
      <div id="collapse_top_ten_pokedex_average_level" class="panel-collapse">
        <div class="panel-body">
          <div id="top_ten_pokedex_average_level"></div>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
	$(function () { 
    var myChart = Highcharts.chart('top_ten_pokemon_winner', {
      chart: {
        type: 'column'
      },
      xAxis: {
        categories: ['Pokemons']
      },
      yAxis: {
        title: {
          text: 'Win Count'
        }
      },
      series: <%= raw @top_ten_pokemon_winner.to_json %>
    });
  });
