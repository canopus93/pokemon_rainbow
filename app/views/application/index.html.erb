<div>
	<img src="https://img.pokemondb.net/artwork/jirachi.jpg" class="center-block">
	<h2 class="text-center">Welcome to Pokemon Rainbow</h2>
</div>

<div class="row">
  <div class="col-sm-4">
    <div class="panel panel-primary">
    	<div class="panel-heading">
    		<a data-toggle="collapse" href="#collapse_top_ten_pokemon_winner">Top Ten Pokemon Winner</a>
    	</div>
    	<div id="collapse_top_ten_pokemon_winner" class="panel-collapse">
    		<div class="panel-body">
    			<div id="top_ten_pokemon_winner"></div>
    		</div>
    	</div>
    </div>
  </div>

  <div class="col-sm-4">
    <div class="panel panel-primary">
    	<div class="panel-heading">
        <a data-toggle="collapse" href="#collapse_top_ten_pokemon_loser">Top Ten Pokemon Loser</a>
      </div>
      <div id="collapse_top_ten_pokemon_loser" class="panel-collapse">
        <div class="panel-body">
          <div id="top_ten_pokemon_loser"></div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-sm-4">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <a data-toggle="collapse" href="#collapse_top_ten_pokemon_surrender">Top Ten Pokemon Surrender</a>
      </div>
      <div id="collapse_top_ten_pokemon_surrender" class="panel-collapse">
        <div class="panel-body">
          <div id="top_ten_pokemon_surrender"></div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-sm-6">
    <div class="panel panel-primary">
    	<div class="panel-heading">
        <a data-toggle="collapse" href="#collapse_top_ten_pokemon_win_rate">Top Ten Pokemon Win Rate</a>
      </div>
      <div id="collapse_top_ten_pokemon_win_rate" class="panel-collapse">
        <div class="panel-body">
          <div id="top_ten_pokemon_win_rate"></div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-sm-6">
    <div class="panel panel-primary">
    	<div class="panel-heading">
        <a data-toggle="collapse" href="#collapse_top_ten_pokemon_lose_rate">Top Ten Pokemon lose Rate</a>
      </div>
      <div id="collapse_top_ten_pokemon_lose_rate" class="panel-collapse">
        <div class="panel-body">
          <div id="top_ten_pokemon_lose_rate"></div>
        </div>
      </div>
    </div>
  </div>
</div>

